<h1>在线用户</h1>
<%= notice %>

<table>
  <tr>
    <th>邮箱</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

  <% @users.each do |user| %>
  <tr>
    <td><%= user.email %></td>
